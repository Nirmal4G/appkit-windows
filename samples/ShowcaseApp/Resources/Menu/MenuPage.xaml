<Page x:Class="CommunityToolkit.Windows.ShowcaseApp.Samples.MenuPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:commands="using:CommunityToolkit.Windows.ShowcaseApp.Menu.Commands"
      xmlns:controls="using:CommunityToolkit.Windows.UI.Controls"
      xmlns:converters="using:CommunityToolkit.Windows.UI.Converters"
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
      mc:Ignorable="d">

    <Page.Resources>
        <ResourceDictionary>
            <commands:NewProjectCommand x:Key="NewProject" />
            <commands:NewFileCommand x:Key="NewFile" />
            <commands:GenericCommand x:Key="GenericCommand" />
            <converters:StringFormatConverter x:Key="StringFormatConverter" />
        </ResourceDictionary>
    </Page.Resources>
</Page>